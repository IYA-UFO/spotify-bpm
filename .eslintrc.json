{
  "root": true,
  "ignorePatterns": ["public/**/*.js"],
  "extends": [
    "eslint:recommended",
    "next",
    "next/core-web-vitals",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "import/no-anonymous-default-export": "off"
  }
}
